{
  "name": "@baqhub/docs-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -r 'yarn next-dev' 'yarn watch-docs'",
    "next-dev": "next dev -p 5178",
    "watch-docs": "node ./src/scripts/watchDocs.js",
    "compress-images": "node ./src/scripts/compressImages.js",
    "build": "tsc --build && next build && yarn compress-images",
    "start": "next start",
    "lint": "next lint",
    "copy-icons": "cp ./src/docs/assets/favicon.svg ./src/app/icon.svg && cp ./src/docs/assets/faviconApple.png ./src/app/apple-icon.png && magick ./src/docs/assets/favicon32.png ./src/docs/assets/favicon16.png ./src/app/favicon.ico"
  },
  "dependencies": {
    "@baqhub/ui": "*",
    "@docsearch/css": "3.9.0",
    "@docsearch/react": "3.9.0",
    "@heroicons/react": "2.2.0",
    "@next/mdx": "15.1.7",
    "default-import": "2.0.8",
    "dom-serializer": "2.0.0",
    "domutils": "3.2.2",
    "feed": "4.2.2",
    "htmlparser2": "10.0.0",
    "lodash": "4.17.21",
    "next": "15.1.7",
    "react": "19.0.0",
    "react-children-utilities": "2.10.0",
    "react-dom": "19.0.0",
    "refractor": "4.8.1",
    "slugify": "1.6.6",
    "tiwi": "1.0.15"
  },
  "devDependencies": {
    "@mdx-js/loader": "3.1.0",
    "@mdx-js/react": "3.1.0",
    "@next/bundle-analyzer": "15.1.7",
    "@svgr/webpack": "8.1.0",
    "@types/lodash": "4.17.15",
    "@types/mdx": "2.0.13",
    "@types/node": "22.13.5",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@types/ws": "8.5.14",
    "autoprefixer": "10.4.20",
    "chokidar": "4.0.3",
    "concurrently": "9.1.2",
    "eslint": "8.57.1",
    "postcss": "8.5.3",
    "rehype-prism-plus": "2.0.0",
    "remark-smartypants": "3.0.2",
    "sharp": "0.33.5",
    "tailwindcss": "4.0.8",
    "typescript": "5.7.3",
    "webpack": "5.98.0",
    "ws": "8.18.1"
  }

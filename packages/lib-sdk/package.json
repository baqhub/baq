{
  "name": "@baqhub/sdk",
  "version": "0.1.12",
  "description": "",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "ts": "tsc --build --watch ./tsconfig.cjs.json ./tsconfig.json",
    "build": "tsc --build ./tsconfig.cjs.json ./tsconfig.json && tsconfig-to-dual-package",
    "build:clean": "rm -rf ./build",
    "publish": "yarn npm publish",
    "test": "yarn node --experimental-vm-modules --no-warnings $(yarn bin jest)"
  },
  "module": "./build/esm/src/index.js",
  "main": "./build/cjs/src/index.js",
  "types": "./build/cjs/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/esm/src/index.js",
      "require": "./build/cjs/src/index.js"
    }
  },
  "author": "Baqhub",
  "license": "MIT",
  "dependencies": {
    "@noble/curves": "^1.4.0",
    "@noble/hashes": "^1.4.0",
    "es6-url-template": "^3.0.2",
    "fp-ts": "^2.16.6",
    "io-ts": "^2.2.21",
    "io-ts-reporters": "^2.0.1",
    "jsonpath-plus": "^9.0.0",
    "lodash": "^4.17.21",
    "tslib": "^2.6.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/lodash": "4.17.6",
    "@types/uuid": "10.0.0",
    "jest": "29.7.0",
    "ts-jest": "29.1.5",
    "ts-jest-resolver": "2.0.1",
    "tsconfig-to-dual-package": "1.2.0",
    "typescript": "5.5.2"
  }
}
